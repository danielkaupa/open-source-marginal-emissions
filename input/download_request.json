{
    "dataset_short_name": "era5_world",
    "dataset_api_name": "reanalysis-era5-single-levels",

    "save_directory": "../data/raw/",
    "skip_overwrite_case-by-case" : 1,
    "download_regardless_of_size" : 0,

    "maximum_concurrent_downloads": 2,
    "maximum_retry_attempts": 6,
    "retry_delay_seconds": 15,

    "era5_world_grid_resolution": 0.25,
    "era5_land_grid_resolution": 0.1,
    "product_type": "reanalysis",
    "data_download_format": "grib",

    "north_latitude_boundary": 50.0,
    "south_latitude_boundary": 24.0,
    "east_longitude_boundary": -66.0,
    "west_longitude_boundary": -125.0,

    "start_date": "2025-01-01",
    "end_date": "2025-06-30",
    "variables": [
        "2m_temperature",
        "total_precipitation",
        "10m_u_component_of_wind",
        "10m_v_component_of_wind",
        "surface_pressure"
    ]
}